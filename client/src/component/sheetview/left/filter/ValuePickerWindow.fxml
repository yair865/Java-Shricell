<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="component.sheetview.left.filter.ValuePickerController" spacing="10" prefWidth="400" prefHeight="300" styleClass="value-picker">
    <children>
        <VBox fx:id="checkboxContainer" spacing="5"/>
        <HBox spacing="10">
            <Button fx:id="okButton" text="OK" onAction="#okListener" />
            <Button fx:id="cancelButton" text="Cancel" onAction="#cancelListener" />
        </HBox>
    </children>
</VBox>
